# - Config file for the LIBBITCOIN package
#
#  LIBBITCOIN_INCLUDE_DIRS - include directories for LIBBITCOIN
#  LIBBITCOIN_LIBRARIES    - libraries to link against

get_filename_component(LIBBITCOIN_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(LIBBITCOIN_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")

include("${LIBBITCOIN_CMAKE_DIR}/libbitcoinTargets.cmake")
set(LIBBITCOIN_LIBRARIES libbitcoin)

find_package(secp256k1 0.0.01 REQUIRED)
